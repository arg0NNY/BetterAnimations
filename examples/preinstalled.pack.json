{
  "name": "preinstalled",
  "author": "arg0NNY",
  "version": "0.0.0",

  "animations": [

    {
      "key": "backdropDark",
      "name": "Dark Blur",
      "meta": {
        "modules": "modalsBackdrop"
      },
      "settings": {
        "duration": true,
        "easing": true,

        "defaults": {
          "duration": 300,
          "easing": "easeInOutSine"
        }
      },
      "animate": {
        "anime": {
          "targets": { "inject": "element" },
          "duration": { "inject": "duration" },
          "easing": { "inject": "easing" },
          "background": {
            "inject": "type",
            "enter": ["rgba(0, 0, 0, 0)", "rgba(0, 0, 0, .7)"],
            "exit": "rgba(0, 0, 0, 0)"
          },
          "backdropFilter": {
            "inject": "type",
            "enter": ["blur(0px)", "blur(2px)"],
            "exit": "blur(0px)"
          }
        }
      }
    }

  ]
}
